       <record id="view_tanmy_product_temp_extension" model="ir.ui.view">
            <field name="model">product.product</field>
            <field name="inherit_id" ref="product.product_normal_form_view"/>


           <field name="type">form</field>
            <field name="arch" type="xml">
                <xpath expr="//div[@name='options']/span[last()]" position="after" >
                    <span class="d-inline-block" style="margin-left:20px" >
                            <label for="kit_template"/>
                            <field name="kit_template" context="{'company_id': company_id}" options="{'no_create': False, 'no_open': False}" />
                    </span>
                    <span class="d-inline-block" style="margin-left:20px" >
                            <label for="prod_category"/>
                            <field name="prod_category" context="{'company_id': company_id}" options="{'no_create': False, 'no_open': False}" />
                    </span>

                    <span class="d-inline-block" style="margin-left:20px" attrs="{'invisible':  [('kit_template', '=', False)]}"  >
                            <label for="favorite"/>
                            <field name="favorite"  />
                    </span>
                </xpath>
            </field>

       </record>

       <record id="favorite_website_menu" model="website.menu">
            <field name="name">Favorites</field>
            <field name="url">/favorites</field>
            <field name="parent_id" ref="website.main_menu"/>
           <field name="sequence" type="int">88</field>

       </record>


